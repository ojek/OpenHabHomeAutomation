    "menucolumns": 1,
    "settings": {
        "theme": "default",
        "additional_stylesheet_url": "/static/custom/themes/custom.css",
        "background_image": "",
        "panel_name": "Home"
    },
    "customwidgets": {
        "SingleGraph": {
            "template": "<div class=\"customWidget\">\n    <div class=\"customHeader\">\n        <div class=\"icon\">\n            <img class=\"invert\" src=\"/static/custom/icons/main/{{config.icon}}.svg\" />\n        </div>\n        <div class=\"name\" ng-class=\"{true: 'itemActive'}[itemValue(config.statusItem)=='ON']\"><span>{{config.title}}</span></div>\n        <div class=\"value\"><span>{{itemValue(config.item) | number:1}}{{config.unit}}</span></div>\n    </div>\n    \n    <div class=\"content iframe\">\n        <div class=\"hidden src\">#loading_{{config.chartlink}}</div>\n        <div class=\"dst\"></div>\n    </div>\n    <div class=\"hidden\" oc-lazy-load=\"['/static/custom/scripts/custom.js']\"></div>\n</div>",
            "settings": [
                {
                    "type": "string",
                    "id": "icon",
                    "label": "Icon"
                },
                {
                    "type": "string",
                    "id": "title",
                    "label": "Title"
                },
                {
                    "type": "item",
                    "id": "item",
                    "label": "Item",
                    "default": "0"
                },
                {
                    "type": "string",
                    "id": "chartlink",
                    "label": "Chart link"
                },
                {
                    "type": "string",
                    "id": "unit",
                    "label": "Unit"
                },
                {
                    "type": "item",
                    "id": "statusItem",
                    "label": "statusItem",
                    "default": "OFF"
                }
            ],
            "name": "IFrame"
        },
        "YahooWeather": {
            "template": "<div class=\"customWidget\">\n    <div class=\"customHeader\">\n        <div class=\"icon\">\n            <img class=\"invert\" src=\"/static/custom/icons/weatherIcons/{{itemValue(config.condition)}}.svg\" />\n        </div>\n        <div class=\"name\"><span>Wroclaw</span></div>\n        <div class=\"value\"><span>{{itemValue(config.temperature) | number:1}}°C</span></div>\n    </div>\n    \n    <div class=\"content info\">\n        <div class=\"row\"><span class=\"left\">Condition</span><span class=\"right\">{{itemValue(config.condition)}}</span></div>\n        <div class=\"row\"><span class=\"left\">Humidity</span><span class=\"right\">{{itemValue(config.humidity)|number:1}}%</span></div>\n        <div class=\"row\"><span class=\"left\">Wind</span><span class=\"right\">{{(itemValue(config.wind)|number:1) + \" km/h\"}}</span></div>\n    </div>\n    <div oc-lazy-load=\"['/static/custom/scripts/custom.js']\"></div>\n</div>",
            "settings": [
                {
                    "type": "item",
                    "id": "condition",
                    "label": "Condition"
                },
                {
                    "type": "item",
                    "id": "humidity",
                    "label": "Humidity"
                },
                {
                    "type": "item",
                    "id": "wind",
                    "label": "Wind"
                },
                {
                    "type": "item",
                    "id": "temperature",
                    "label": "Temperature"
                }
            ]
        },
        "ESP8266": {
            "template": "<div class=\"customWidget\">\n    <div class=\"customHeader\">\n        <div class=\"icon\">\n            <img class=\"invert\" src=\"/static/custom/icons/main/chip.svg\" />\n        </div>\n        <div class=\"name\"><span>ESP8266 - Main</span></div>\n        <div class=\"value\"><span></span></div>\n    </div>\n    \n    <div class=\"content info\">\n        <div class=\"row\"><span class=\"left\">Uptime</span><span class=\"right\">{{((itemValue(config.uptime)/86400)|number:2) + \" Days\"}}</span></div>\n        <div class=\"row\"><span class=\"left\">Memory free</span><span class=\"right\">{{((itemValue(config.memfree)/1000)|number:1) + \" KiB\"}}</span></div>\n        <div class=\"row\"><span class=\"left\">CPU Frequency</span><span class=\"right\">{{(itemValue(config.cpufreq)) + \" MHz\"}}</span></div>\n    </div>\n    <div oc-lazy-load=\"['/static/custom/scripts/custom.js']\"></div>\n</div>",
            "settings": [
                {
                    "type": "item",
                    "id": "uptime",
                    "label": "Uptime"
                },
                {
                    "type": "item",
                    "id": "memfree",
                    "label": "Memfree"
                },
                {
                    "type": "item",
                    "id": "cpufreq",
                    "label": "Cpufreq"
                }
            ]
        },
        "Slider": {
            "template": "<div class=\"customWidget\">\n    <div class=\"customHeader\">\n        <div class=\"icon\">\n            <img class=\"invert\" src=\"/static/custom/icons/main/{{config.icon}}.svg\" />\n        </div>\n        <div class=\"name\"><span>{{config.title}}</span></div>\n        <div class=\"value\"><span>{{itemValue(config.item) | number:1}}{{config.unit}}</span></div>\n    </div>\n    \n    <div class=\"content\">\n        <input type=\"range\" min=\"{{config.min}}\" max=\"{{config.max}}\" value=\"{{config.default}}\" class=\"slider\" id=\"{{config.id}}\">\n    </div>\n    <div oc-lazy-load=\"['/static/custom/scripts/custom.js']\"></div>\n</div>",
            "settings": [
                {
                    "type": "string",
                    "id": "icon",
                    "label": "icon"
                },
                {
                    "type": "string",
                    "id": "title",
                    "label": "title"
                },
                {
                    "type": "item",
                    "id": "item",
                    "label": "item"
                },
                {
                    "type": "string",
                    "id": "unit",
                    "label": "unit"
                },
                {
                    "type": "string",
                    "id": "min",
                    "label": "min"
                },
                {
                    "type": "string",
                    "id": "max",
                    "label": "max"
                },
                {
                    "type": "string",
                    "id": "default",
                    "label": "default",
                    "default": ""
                },
                {
                    "type": "string",
                    "id": "id",
                    "label": "id"
                }
            ]
        }
    }
}